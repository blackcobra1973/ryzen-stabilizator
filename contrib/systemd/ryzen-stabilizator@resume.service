[Unit]
Description=Ryzen Stabilizator Tabajara - Resume
After=suspend.target
PartOf=ryzen-stabilizator.target

[Service]
Type=simple
User=root
Group=root
ExecStart=/usr/bin/ryzen-stabilizator --config=/etc/ryzen-stabilizator/ryzen-stabilizator.toml --profile=resume

[Install]
WantedBy=suspend.target
